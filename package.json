{
  "name": "coinspot-ts-client",
  "version": "1.0.0",
  "description": "Modern TypeScript client for the CoinSpot v2 API with retries, rate limiting, and zod validation.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "coinspot",
    "crypto",
    "api-client",
    "typescript",
    "zod"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JesseVent/coinspot-ts-client.git"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "compile": "tsc --noEmit",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "test": "echo \"No tests yet\"",
    "probe": "ts-node scripts/coinspot_probe.ts"
  },
  "dependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.17.9",
    "prettier": "^3.3.3",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
