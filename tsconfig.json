{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["es2020"],
    "types": ["node"],
    "rootDir": ".",
    "outDir": "dist",
    "declaration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["*.ts", "binance-schemas.ts", "binance-wrapper.ts", "client.ts", "index.ts", "schema-normalizer.ts", "schemas.ts"],
  "exclude": ["dist", "node_modules", "scripts"]
}
